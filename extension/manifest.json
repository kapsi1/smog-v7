{
  "manifest_version": 2,
  "name": "Smog Małopolska",
  "description": "Poziom smogu w Małopolsce",
  "version": "12",
  "homepage_url": "https://github.com/kapsi1/smog-v7",
  "background": {"page": "speed-dial.html"},
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "speeddial",
    "storage",
    "http://default-environment.e2uzb9ftv3.eu-west-1.elasticbeanstalk.com/"
  ],
  "speeddial": {
    "url": "http://monitoring.krakow.pios.gov.pl/",
    "title": "Smog Małopolska",
    "size_mode": "adapt"
  }
}