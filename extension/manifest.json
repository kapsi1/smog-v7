{
  "manifest_version": 2,
  "name": "Smog Małopolska",
  "description": "Poziom smogu w Małopolsce",
  "version": "7",
  "homepage_url": "https://github.com/kapsi1/Smog",
  "background": {"page": "speed-dial.html"},
  "options_page": "options.html",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "speeddial",
    "http://smog-server.herokuapp.com/"
  ],
  "speeddial": {
    "url": "http://monitoring.krakow.pios.gov.pl/",
    "title": "Smog Małopolska",
    "size_mode": "adapt"
  }
}